import pandas as pd
df = pd.read_excel(r"excel_file.xslx")
df = df.truncate(before=120, after=149)
red = df.loc[df['Uso'] == "Florero"]['#'].tolist()
aux = list()
# [1, 2, 4, 54, 65, 8]
for i in range(len(red)):
    for j in range(i+1, len(red)):
        aux.append([red[i], red[j]])
aux = ' '.join(map(str, aux))
aux = aux.replace("[", "(")
print(aux.replace("]", ")"))
